Процедура ПередЗаписью(Отказ)
	
	Если ОбменДанными.Загрузка Тогда
		Возврат;
	КонецЕсли;
		
	ЗаполнитьШаблон();	
КонецПроцедуры

Процедура ЗаполнитьШаблон() Экспорт
	Если Не Найти(Шаблон, Код) Тогда
		Шаблон = "";
	КонецЕсли;
	
	Если Не ЗначениеЗаполнено(Шаблон) Тогда	
		Шаблон = Код + "(.*)";
	КонецЕсли;
	
КонецПроцедуры

