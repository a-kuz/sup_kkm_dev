		<field xsi:type="DataSetFieldField">
			<dataPath>Контрагент</dataPath>
			<field>Контрагент</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СебестоимостьЗаЕдНоменклатуры</dataPath>
			<field>СебестоимостьЗаЕдНоменклатуры</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаСебестоимость</dataPath>
			<field>СуммаСебестоимость</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ОфициантДозаказа</dataPath>
			<field>ОфициантДозаказа</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Официант дозаказа</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ТипЦен</dataPath>
			<field>ТипЦен</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Станция</dataPath>
			<field>Станция</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Станция</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЗаказТовары.Ссылка КАК Заказ,
	ЗаказТовары.Товар КАК Товар,
	ЗаказТовары.Количество КАК КоличествоРеализовано,
	ВЫБОР
		КОГДА &amp;Сраспределением
			ТОГДА ЗаказТовары.СуммаРеализации
		ИНАЧЕ ЗаказТовары.Сумма
	КОНЕЦ КАК СуммаРеализации,
	ЗаказТовары.Количество КАК КоличествоПриготовлено,
	ЗаказТовары.Автор КАК ОфициантДозаказа,
	ЗаказТовары.Ссылка.Автор КАК Официант,
	ЗаказТовары.Ссылка.Смена КАК Смена,
	ЗаказТовары.Ссылка.МестоРеализации КАК МестоРеализации,
	ЗаказТовары.Ссылка.Клиент КАК Клиент,
	ЗаказТовары.Ссылка.Дата КАК Период,
	ЗаказТовары.Товар.Категория КАК Категория,
	ЗаказТовары.Товар.ГруппаПечати КАК ГруппаПечати,
	ЗаказТовары.Товар.ЕдиницаИзмерения КАК ЕдиницаИзмерения,
	ЗаказТовары.Ссылка.Клиент.Контрагент КАК Контрагент,
	ЗаказТовары.Ссылка.МестоРеализации.ИнформационнаяБаза КАК ИнформационнаяБаза,
	ЗаказТовары.ТипЦен,
	ЗаказТоварыДопИнф.Станция,
	ЗаказТоварыДопИнф.АвторГотово,
	ЗаказТоварыДопИнф.РабочееМесто
ПОМЕСТИТЬ ДокументЗаказТовары
ИЗ
	Документ.Заказ.Товары КАК ЗаказТовары
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.ЗаказТоварыДопИнф КАК ЗаказТоварыДопИнф
		ПО ЗаказТовары.Ссылка = ЗаказТоварыДопИнф.Заказ
			И ЗаказТовары.ИдСтроки = ЗаказТоварыДопИнф.ИдСтроки
ГДЕ
	ЗаказТовары.Ссылка.ПометкаУдаления = ЛОЖЬ
	И ВЫБОР
			КОГДА &amp;ПараметрДата
				ТОГДА ЗаказТовары.Ссылка.Дата МЕЖДУ &amp;ДатаС И &amp;ДатаПо
			ИНАЧЕ ЗаказТовары.Ссылка.Смена В (&amp;МассивСмен)
		КОНЕЦ
	И ЗаказТовары.Количество &lt;&gt; 0

ИНДЕКСИРОВАТЬ ПО
	Период,
	Товар
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДокументУдаление.Заказ КАК Заказ,
	ДокументУдаление.Заказ.Дата КАК ДатаЗаказа,
	ДокументУдаление.Заказ.Клиент КАК Клиент,
	ДокументУдаление.Заказ.Клиент.Контрагент КАК Контрагент,
	ДокументУдаление.МестоРеализации КАК МестоРеализации,
	ДокументУдаление.МестоРеализации.ИнформационнаяБаза КАК ИнформационнаяБаза,
	ДокументУдаление.Смена КАК Смена,
	ДокументУдаление.Автор КАК Автор,
	ДокументУдаление.Товар КАК Товар,
	ДокументУдаление.Товар.Категория КАК Категория,
	ДокументУдаление.Товар.ГруппаПечати КАК ГруппаПечати,
	ДокументУдаление.Товар.ЕдиницаИзмерения КАК ЕдиницаИзмерения,
	ДокументУдаление.Количество КАК Количество,
	ДокументУдаление.Дата КАК Дата,
	ДокументУдаление.Сумма КАК Сумма,
	ДокументУдаление.ТипЦен
ПОМЕСТИТЬ Удаление
ИЗ
	Документ.Удаление КАК ДокументУдаление
ГДЕ
	ДокументУдаление.ПометкаУдаления = ЛОЖЬ
	И ДокументУдаление.Причина.Списание = ИСТИНА
	И ВЫБОР
			КОГДА &amp;ПараметрДата
				ТОГДА ДокументУдаление.Дата МЕЖДУ &amp;ДатаС И &amp;ДатаПо
			ИНАЧЕ ДокументУдаление.Смена В (&amp;МассивСмен)
		КОНЕЦ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Товары.Ссылка КАК Ссылка,
	Товары.Ссылка.Заказ КАК Заказ,
	Товары.Ссылка.Автор КАК Автор,
	Товары.Ссылка.Смена КАК Смена,
	Товары.Ссылка.МестоРеализации КАК МестоРеализации,
	Товары.Ссылка.Клиент КАК Клиент,
	Товары.Ссылка.Дата КАК Дата,
	Товары.Ссылка.Клиент.Контрагент КАК Контрагент,
	Товары.Ссылка.МестоРеализации.ИнформационнаяБаза КАК ИнформационнаяБаза,
	Товары.Товар КАК Товар,
	Товары.Товар.Категория КАК Категория,
	Товары.Товар.ГруппаПечати КАК ГруппаПечати,
	Товары.Товар.ЕдиницаИзмерения КАК ЕдиницаИзмерения,
	Товары.Количество КАК Количество,
	Товары.Сумма КАК Сумма,
	Товары.Ссылка.Заказ.Дата КАК ДатаЗаказа,
	Товары.ТипЦен
ПОМЕСТИТЬ ВозвратТовары
ИЗ
	Документ.Возврат.Товары КАК Товары
ГДЕ
	Товары.Ссылка.ПометкаУдаления = ЛОЖЬ
	И Товары.Ссылка.Причина.Списание = ИСТИНА
	И ВЫБОР
			КОГДА &amp;ПараметрДата
				ТОГДА Товары.Ссылка.Дата МЕЖДУ &amp;ДатаС И &amp;ДатаПо
			ИНАЧЕ Товары.Ссылка.Смена В (&amp;МассивСмен)
		КОНЕЦ
;

