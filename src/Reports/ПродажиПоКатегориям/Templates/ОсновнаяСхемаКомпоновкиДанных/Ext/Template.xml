		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЗаказТовары.Товар.Категория КАК Категория,
	ЗаказТовары.Товар.ГруппаПечати КАК ГруппаПечати,
	ЗаказТовары.ГруппаОплаты КАК ГруппаОплаты,
	ЗаказТовары.Количество,
	ЗаказТовары.Сумма,
	ЗаказТовары.Ссылка.МестоРеализации,
	НАЧАЛОПЕРИОДА(ЗаказТовары.Ссылка.Дата, ГОД) КАК Год,
	НАЧАЛОПЕРИОДА(ЗаказТовары.Ссылка.Дата, КВАРТАЛ) КАК Квартал,
	НАЧАЛОПЕРИОДА(ЗаказТовары.Ссылка.Дата, МЕСЯЦ) КАК Месяц,
	НАЧАЛОПЕРИОДА(ЗаказТовары.Ссылка.Дата, НЕДЕЛЯ) КАК Неделя,
	НАЧАЛОПЕРИОДА(ЗаказТовары.Ссылка.Дата, ДЕНЬ) КАК День,
	ДЕНЬНЕДЕЛИ(ЗаказТовары.Ссылка.Дата) КАК ДНИНЕДЕЛИ,
	ЗаказТовары.Ссылка.Дата КАК Период,
	ЗаказТовары.СуммаРеализации КАК СуммаСоСкидкой,
	ЗаказТовары.Сумма - ЗаказТовары.СуммаРеализации КАК СкидкаНаценка,
	ЗаказТовары.Ссылка.Смена,
	ЗаказТовары.Количество * 100 / &amp;КолВсего КАК ПроцентКоличество,
	ЗаказТовары.СуммаРеализации * 100 / &amp;СуммаВсего КАК ПроцентСумма,
	ЗаказТовары.ТипЦен
ИЗ
	Документ.Заказ.Товары КАК ЗаказТовары
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ЗаказДопИнф КАК ЗаказДопИнф
		ПО ЗаказТовары.Ссылка = ЗаказДопИнф.Заказ
ГДЕ
	ЗаказТовары.Ссылка.ПометкаУдаления = ЛОЖЬ
	И ЗаказТовары.Количество &lt;&gt; 0
	И ВЫБОР
			КОГДА &amp;ПараметрДата
				ТОГДА ЗаказТовары.Ссылка.Дата МЕЖДУ &amp;ДатаС И &amp;ДатаПо
			ИНАЧЕ ЗаказТовары.Ссылка.Смена В (&amp;МассивСмен)
		КОНЕЦ
	И ВЫБОР
			КОГДА &amp;ПоЗаказам = 1
				ТОГДА ЗаказДопИнф.ПротоколРасчетов &lt;&gt; ЗНАЧЕНИЕ(Документ.ПротоколРасчетов.ПустаяСсылка)
			ИНАЧЕ ИСТИНА
		КОНЕЦ</query>
	</dataSet>
	<totalField>
		<dataPath>Количество</dataPath>
		<expression>Строка(Формат(Сумма(Количество), "ЧДЦ=3")) + "           " + Строка(Формат(Сумма(Количество)*100./&amp;КолВсего, "ЧДЦ=2")) + "%"</expression>
	</totalField>
	<totalField>
		<dataPath>Сумма</dataPath>
		<expression>Сумма(Сумма)</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаСоСкидкой</dataPath>
		<expression>Строка(Формат(Сумма(СуммаСоСкидкой), "ЧДЦ=2")) + "           " + Строка(Формат(Сумма(СуммаСоСкидкой)*100/&amp;СуммаВсего, "ЧДЦ=2")) + "%"</expression>
	</totalField>
	<totalField>
		<dataPath>СкидкаНаценка</dataPath>
		<expression>Сумма((-1)*СкидкаНаценка)</expression>
	</totalField>
