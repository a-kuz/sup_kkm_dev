			<field>ТипЦен</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>НомерКартыЛояльности</dataPath>
			<field>НомерКартыЛояльности</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Номер карты КеГеЛьБУМ</v8:content>
