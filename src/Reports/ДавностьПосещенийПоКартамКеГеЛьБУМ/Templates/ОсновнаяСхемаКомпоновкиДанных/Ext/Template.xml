			<dataPath>Товар</dataPath>
			<field>Товар</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ЕстьЗаказыСОбедами</dataPath>
			<field>ЕстьЗаказыСОбедами</field>
